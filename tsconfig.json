{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2019"],
    "module": "commonjs",
    "allowJs": false,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
